<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.foodtinder">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_add_circle"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_add_circle_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignInActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity android:name=".SignOutActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity android:name=".CreateGroupActivity" />
        <activity android:name=".TestEvent" />
        <activity android:name=".EventSelectionActivity" />
        <activity android:name=".EventFinalActivity" />
        <activity android:name=".DisplayGroupLink"/>
        <activity android:name=".GroupLandingPage"/>
        <activity android:name=".Swiping"/>


    </application>

</manifest>